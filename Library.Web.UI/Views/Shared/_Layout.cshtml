                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                                <span class="icon-bar"></span>
                            </button>
                            <a class="navbar-brand" href="#"></a>
                        </div>
                        <!-- Collect the nav links, forms, and other content for toggling -->
                        <div class="collapse navbar-collapse" id="bs-megadropdown-tabs">
                            <ul class="nav navbar-nav nav_1">
                                <li><a href="/Home/Index">Home</a></li>
                                <li><a href="#" class="dropdown-toggle" data-toggle="dropdown">Department<span class="caret"></span></a>                                 
                                        <ul class="dropdown-menu">
                                            <li><a href="/BookDisplay/Index/IT">IT</a></li>
                                            <li><a href="/BookDisplay/Index/CSE">CSE</a></li>
                                            <li><a href="/BookDisplay/Index/ECE">ECE</a></li>
                                            <li><a href="/BookDisplay/Index/EEE">EEE</a></li>
                                            <li><a href="/BookDisplay/Index/ALL">ALL</a></li>
                                            
                                        </ul>                                    
                                </li>                              
                              
                                <li><a href="#">Read-Book(soon)</a></li>
                               
                                <li class="last"><a href="/Login/Logout">Logout</a></li>
                                <li><a href="account.html"></a></li>
                            </ul>
                        </div><!-- /.navbar-collapse -->

                    </nav>
                </div>


                <div class="clearfix"> </div>
               
                <div id="small-dialog" class="mfp-hide">
                    <div class="search-top">
                        <div class="login">
                            <form action="#" method="post">
                                <input type="submit" value="">
                                <input type="text" name="search" value="Type something..." onfocus="this.value = '';" onblur="if (this.value == '') {this.value = '';}">

                            </form>
                        </div>
                        <p>	java</p>
                    </div>
                </div>
                <script>
						$(document).ready(function() {
						$('.popup-with-zoom-anim').magnificPopup({
							type: 'inline',
							fixedContentPos: false,
							fixedBgPos: true,
							overflowY: 'auto',
							closeBtnInside: true,
							preloader: false,
							midClick: true,
							removalDelay: 300,
							mainClass: 'my-mfp-zoom-in'
						});

						});
                </script>
                <!---->
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
